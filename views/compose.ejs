<%- include('partials/header'); -%>
    <h1>Compose</h1>
   <form action='/compose' method='post'>
    <div class='form-group'>
    <div class="mb-3">
    <label for="publish_box" class="form-label">Title</label>
       <input  class='form-control' type='text' name='publish_box'>
    </div>
       <div class="mb-3">

        <label for="postBody" class="form-label">Post</label>
        <textarea class="form-control" name='postBody' id="postBody" rows="5"></textarea>
      </div>
    </div>
       <button type='submit' class='btn btn-primary' name='publish'>Publish</button>
   </form>

<%- include('partials/footer'); -%>